@model IEnumerable<LearningManagementSystem.Data.Tranfer>

@{
    ViewData["Title"] = "Index";
}

<h3>انواع التنقل</h3>

<p>
    <a asp-action="Create">انشاء جديد</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                اسم التنقل
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TransferName)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.TransferId">التعديل</a> |
                <a asp-action="Details" asp-route-id="@item.TransferId">التفاصيل</a> |
                <a asp-action="Delete" asp-route-id="@item.TransferId">الحذف</a>
            </td>
        </tr>
}
    </tbody>
</table>
