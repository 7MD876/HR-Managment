@model LearningManagementSystem.Data.ECourse

@{
    ViewData["Title"] = "Delete";
}




<div>
    <h4>حذف دورة</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            تاريخ البداية
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DateOfStart)
        </dd>
        <dt class = "col-sm-2">
            تاريخ النهاية
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DateOfEnd)
        </dd>
        <dt class = "col-sm-2">
            التقييم
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Rating)
        </dd>
        <dt class = "col-sm-2">
             الدورة
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Course.CourseName)
        </dd>
        <dt class = "col-sm-2">
            الرقم الوظيفي
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdemployeesNavigation.Employeenumber)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="IdemployeesCourses" />
        <input type="submit" value="حذف" class="btn btn-danger" /> |
        <a asp-controller="Employees" asp-action="Details" asp-route-id=" @Model.Idemployees"> <i class="bx bx-detail"></i>العودة لبيانات الموظف</a>
    </form>
</div>
