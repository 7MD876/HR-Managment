@model IEnumerable<LearningManagementSystem.Data.ECourse>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    @* <a asp-action="Create">Create New</a> *@
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DateOfStart)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DateOfEnd)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Rating)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Course)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdemployeesNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                    @Html.DisplayFor(modelItem => item.DateOfStart)
            </td>
            <td>
                    @Html.DisplayFor(modelItem => item.DateOfEnd)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Rating)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Course.CourseName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdemployeesNavigation.Name)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdemployeesCourses">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdemployeesCourses">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdemployeesCourses">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
