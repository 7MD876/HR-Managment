@model IEnumerable<LearningManagementSystem.Data.Jop>

@{
    ViewData["Title"] = "Index";
}

<h3>مسميات الوظائف</h3>

<p>
    <a asp-action="Create">اضافة جديد</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
               مسمى الوظيفة
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.JopName)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Idjops">تعديل</a> |
                <a asp-action="Details" asp-route-id="@item.Idjops">التفاصيل</a> |
                <a asp-action="Delete" asp-route-id="@item.Idjops">حذف</a>
            </td>
        </tr>
}
    </tbody>
</table>
