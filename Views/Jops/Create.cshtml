@model LearningManagementSystem.Data.Jop

@{
    ViewData["Title"] = "Create";
}

<h1>اضافة وظائف</h1>

<h4>Jop</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="JopName" class="control-label"></label>
                <input asp-for="JopName" class="form-control" />
                <span asp-validation-for="JopName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">العودة لقائمة الوظائف<i class="bx bx-arrow-back"></i></a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
