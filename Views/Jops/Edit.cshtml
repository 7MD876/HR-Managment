@model LearningManagementSystem.Data.Jop

@{
    ViewData["Title"] = "Edit";
}

<h3>تعديل مسمى الوظيفة</h3>


<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Idjops" />
            <div class="form-group">
                <label  class="control-label">مسمى الوظيفة</label>
                <input asp-for="JopName" class="form-control" />
                <span asp-validation-for="JopName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="حفظ" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">العودة لقائمة الوظائف<i class="bx bx-arrow-back"></i></a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
