@model LearningManagementSystem.Data.Medel

@{
    ViewData["Title"] = "Delete";
}

<h3>الحذف</h3>


<div>
    
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            اسم النوط / الوسام
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.MedelsName)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="Idmedels" />
        <input type="submit" value="حذف" class="btn btn-danger" /> |
        <a asp-action="Index">العودة الى القائمة السابقة</a>
    </form>
</div>
