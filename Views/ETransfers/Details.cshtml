@model LearningManagementSystem.Data.ETransfer

@{
    ViewData["Title"] = "Details";
}

<h1>التفاصيل</h1>

<div>
    
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            الوحده المنقول منها
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FromUnit)
        </dd>
        <dt class = "col-sm-2">
           تاريخ التنقل
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.TransferDate)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.IdEmployeesNavigation)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdEmployeesNavigation.IdEmployees)
        </dd>
        <dt class = "col-sm-2">
            التنقل
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Transfer)
        </dd>
    </dl>
</div>

<div>
    <a asp-action="Edit" asp-route-id="@Model.IdTransfer">Edit</a> |
    <a asp-controller="Employees" asp-action="Details" asp-route-id="@Model.IdEmployees"><i class="bx bx-detail"></i>العودة لبيانات الموظف</a>
</div>
