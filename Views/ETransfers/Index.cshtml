@model IEnumerable<LearningManagementSystem.Data.ETransfer>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TransferId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TransferDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdEmployeesNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdTransferNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TransferId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TransferDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdEmployeesNavigation.IdEmployees)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdTransferNavigation.TransferId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdTransfer">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdTransfer">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdTransfer">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
